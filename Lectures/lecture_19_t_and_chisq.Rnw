\begin{frame}[fragile]
\frametitle{Consider the $\chi^2$}
\begin{itemize}
  \item{Test {\bf Goodness of fit} between observed and expected values}
  \pause
  \hfill \\
  \item{If there is no difference, deviations should be normally distributed noise}
  \pause
  \hfill \\
  \item{Summing up + and - differences is problematic - so we square!}
  \pause
  \hfill \\
  \item{The square of a normal distribution is the $\chi^2$ distribution!}
  \pause
  \hfill \\
  \item{The $\chi^2$ is defined by degrees of freedom = n-1!}
\end{itemize}
\end{frame}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\begin{frame}[fragile]
\frametitle{Consider the $\chi^2$}
\begin{center}
$\chi^2 = \sum\frac{\displaystyle(O-E)^2}{E}$

<<chisq, echo=FALSE, fig.height=6, fig.width=8, out.height="0.6\\textwidth", out.width="0.8\\textwidth">>=
chdata<-expand.grid(df=seq(1,9,3), x=seq(0,15,.05))

qplot(x, dchisq(x, df=df), data=chdata, geom="line", color=factor(df), group=df, size=I(1.5), lty=factor(df)) +
  theme_bw() +
  ylim(0,0.5) +
  xlab("Y") +
  ylab("Probability Density") +
  geom_vline(xintercept=0) +
  scale_color_discrete("Degrees of Freedom") 
@

\end{center}
\end{frame}

