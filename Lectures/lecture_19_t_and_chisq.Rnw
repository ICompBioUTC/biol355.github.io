\begin{frame}[fragile]
\frametitle{Exercise: Gimme a T (function)}
\begin{center}
  $T = \frac{\bar{Y} - \mu}{SE_{\bar{Y}}}$

<<tdist, echo=FALSE, out.width="0.5\\textwidth", out.height="0.5\\textwidth">>=
library(ggplot2)
x<-seq(-3,3,.01)
y<-dt(seq(-3,3,.01), 25)
