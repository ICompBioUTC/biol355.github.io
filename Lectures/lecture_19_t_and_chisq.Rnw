\begin{frame}[fragile]
\frametitle{Sample's Aren't Normal: Student's T}
\begin{center}
<<tsamp, echo=FALSE, out.width="0.7\\textwidth", out.height="0.7\\textwidth">>=

makeTPlot <- function(n, ...){
  vec <- rnorm(n)
  densLines <- density(vec)
  ymax <- max(c(0.4, max(densLines$y)))
  plot(densLines, col="blue", lwd=4, lty=2, xlim=c(-4,4), ylim=c(0,ymax), 
       xlab="X", cex.lab=1.4, cex.axis=1.4, cex.main=1.8, ...)
  
  x <- seq(-4,4,.01)
  matplot(x, dnorm(x), type="l", add=T, col="red", lwd=5)
}

set.seed(31415)
par(mfrow=c(2,2))
makeTPlot(3, main="N=3")
makeTPlot(5, main="N=5")
makeTPlot(10, main="N=10")
makeTPlot(20, main="N=20")
par(mfrow=c(1,1))
@
\end{center}
\end{frame}

